<nav>
  <h1>All Companies</h1>
  <p><%= link_to "Back Home", root_path %><p>
</nav>

<div class="companies-container">
<% @companies.each do |company| %>
<article class="each-company">
  <h3><%= link_to company.name, company_path(company) %></h3>
  <p>
    <%= link_to "Edit", edit_company_path(company) %>
    <%= link_to "Delete", company_path(company), method: "delete" %>
  </p>
</article>
<% end %>
</div>
